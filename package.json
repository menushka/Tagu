{
  "name": "tagu",
  "productName": "Tagu",
  "version": "1.0.0",
  "description": "An app to manage / view your files using tags",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --force",
    "lint-fix": "tslint --project tsconfig.json --force --fix"
  },
  "keywords": [],
  "author": "Menushka Weeratunga <contact@menushka.ca>",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "tagu"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "tagu"
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.24.0",
    "@blueprintjs/icons": "^3.14.0",
    "@blueprintjs/select": "^3.12.0",
    "dotenv": "^8.2.0",
    "electron": "^8.0.3",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-dropzone": "^10.2.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^7.2.0",
    "react-split-pane": "^0.1.89",
    "realm": "^4.0.0-beta.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-redux": "^7.1.7",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  }
}
