{
  "name": "tagu",
  "productName": "Tagu",
  "version": "1.0.0",
  "description": "An app to manage / view your files using tags",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64",
    "rebuild-sqlite": "./node_modules/.bin/electron-rebuild -f -w better-sqlite3"
  },
  "build": {
    "appId": "ca.menushka.tagu",
    "productName": "Tagu",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Menushka Weeratunga <contact@menushka.ca>",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.24.0",
    "@blueprintjs/icons": "^3.14.0",
    "@blueprintjs/select": "^3.12.0",
    "better-sqlite3": "^5.4.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "react-dropzone": "^10.2.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^7.2.0",
    "react-split-pane": "^0.1.89",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.11.1",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.155",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "acorn": "^6.4.0",
    "ajv": "^6.10.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.11.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "node-loader": "^1.0.1",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
